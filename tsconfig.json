{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitAny": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": false,
    "strict": true,
    "target": "ESNext",
    "typeRoots": ["node_modules/@types", "gen-nodejs"]
  },
  "include": ["parquet.ts", "lib/**/*", "gen-nodejs/*", "browser/*.ts"],
  "files": ["lib/custom.d.ts"]
}
